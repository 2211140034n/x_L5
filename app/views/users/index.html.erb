<% @users.each do |e| %>
  <p>uid: <%= e.uid %>, pass: <%= e.pass %></p>
  <%= link_to '削除', user_path(e), data: { turbo_method: :delete } %>
<% end %>

<%= form_with model: User, local: true do |f| %>
  <div>
    <%= f.label :uid, 'UID' %>
    <%= f.text_field :uid %>
  </div>
  <div>
    <%= f.label :pass, 'パスワード' %>
    <%= f.password_field :pass %>
  </div>
  <%= f.submit '登録' %>
<% end %>

<%= link_to 'ログイン', root_path %>